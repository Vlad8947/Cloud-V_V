<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="580.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.RootController">
   <bottom>
      <Label fx:id="statusLabel" alignment="CENTER" prefHeight="21.0" prefWidth="241.0" text="Label" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="5.0" />
         </BorderPane.margin>
      </Label>
   </bottom>
   <top>
      <HBox alignment="CENTER" prefHeight="57.0" prefWidth="520.0" spacing="30.0" BorderPane.alignment="CENTER">
         <children>
            <ToggleButton fx:id="toggleFilesOnCloud" mnemonicParsing="false" onAction="#listViewOnRoot" text="Files on Cloud" />
            <ToggleButton fx:id="toggleAddFilesToCloud" mnemonicParsing="false" onAction="#addFilesOnRoot" text="Add files to Cloud" />
         </children>
      </HBox>
   </top>
</BorderPane>
